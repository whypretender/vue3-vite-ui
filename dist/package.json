{
  "name": "vue3-vite-ui",
  "version": "1.0.16",
  "description": "vue3 vite ui test",
  "main": "vue3-vite-ui.umd.js",
  "module": "vue3-vite-ui.esm.js",
  "typings": "src/@types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint1": "",
    "lint": "eslint --fix --ext .ts,.vue src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "build": " npm run build:components &&  npm run tsc:build ",
    "build:all": "vite build",
    "build:components": "esno ./scripts/build.ts",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "tsc:build": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck",
    "dts:build": " npm run build:components "
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "vite": "^3.1.8",
    "vitepress-demoblock": "^1.0.3",
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@iconify-json/ic": "1.1.4",
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitejs/plugin-vue-jsx": "2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "terser": "^5.4.0",
    "unocss": "0.45.6",
    "vite-plugin-dts": "^1.6.6",
    "vitepress": "0.22.4",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue-tsc": "^1.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/whypretender/smarty-ui-vite.git"
  }
}